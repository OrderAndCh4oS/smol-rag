      <data key="d3">excerpt_id_923669a3de7d0f5bc1f46d1c2b4d64f3:|:excerpt_id_edc83363b14236c3084c4ecf14118ae1</data>
    </node>
    <node id="Example API Requests">
      <data key="d1">example</data>
      <data key="d2">Example API Requests are illustrative sample calls demonstrating how to interact with an API, showcasing typical request formats, parameters, and usage scenarios to guide developers.</data>
      <data key="d3">excerpt_id_8e486f4de3d428521f03db8722d535b5</data>
